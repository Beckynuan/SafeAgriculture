<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.capston">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Capston"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".welcomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name="Care.CareActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="Cleaning.EquipmentActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="Disposal.DisposalActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="Spraying.SprayingActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="Mixing.MixingActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="Storage.StorageActivity"
            android:theme="@style/Theme.Capston.NoAction"/>

        <activity android:name="My_plan.Snails.SnailsActivity"
            android:theme="@style/Theme.Capston.NoAction">
        </activity>
        <activity android:name="My_plan.Rats.RatsActivity"
            android:theme="@style/Theme.Capston.NoAction"/>
        <activity
            android:name="My_plan.Weeds.WeedsActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Disease.DiseasesActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Insect.InsectsActivity"
            android:theme="@style/Theme.Capston.NoAction" />

        <activity
            android:name="My_plan.Insect.Item1_Activity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.MyPlanActivity"
            android:theme="@style/Theme.Capston.NoAction" />

        <activity android:name="Purchase.PurchaseActivity"
            android:theme="@style/Theme.Capston.NoAction"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.Capston.NoAction">

        </activity>
    </application>

</manifest>