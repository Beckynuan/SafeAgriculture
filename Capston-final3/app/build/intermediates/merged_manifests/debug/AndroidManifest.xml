<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capston"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Capston"
        android:usesCleartextTraffic="true" >
        <activity android:name="com.example.capston.welcomePage" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="Care.CareActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Cleaning.EquipmentActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Disposal.DisposalActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Spraying.SprayingActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Mixing.MixingActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Storage.StorageActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Snails.SnailsActivity"
            android:theme="@style/Theme.Capston.NoAction" >
        </activity>
        <activity
            android:name="My_plan.Rats.RatsActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Weeds.WeedsActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Disease.DiseasesActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Insect.InsectsActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.Insect.Item1_Activity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="My_plan.MyPlanActivity"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="Purchase.PurchaseActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@style/Theme.Capston.NoAction" />
        <activity
            android:name="com.example.capston.MainActivity"
            android:theme="@style/Theme.Capston.NoAction" >
        </activity>

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="com.example.capston.com.squareup.picasso"
            android:exported="false" />
    </application>

</manifest>